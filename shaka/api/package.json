  "scripts": {
    "build": "npm run clean && tsc --project tsconfig.build.json",
    "clean": "rimraf dist/",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix --cache",
    "rebuild": "rimraf tmp/output/ tmp/segments/ tmp/converted-videos/ tmp/merged-videos/ && mkdir \"tmp/output\"  \"tmp/segments\" \"tmp/converted-videos\" \"tmp/merged-videos\"",
    "start": "node dist/index.js",
    "start:local": "npm run rebuild && cross-env TS_NODE_PROJECT=tsconfig.json TS_NODE_FILES=true ts-node ./src/index.ts",
    "typecheck": "tsc --project tsconfig.json --noEmit",
    "test": "npm run rebuild && cross-env TS_NODE_PROJECT=tsconfig.json TS_NODE_FILES=true ts-node ./src/test.ts",
    "test:segment": "npm run rebuild && cross-env TS_NODE_PROJECT=tsconfig.json TS_NODE_FILES=true ts-node ./src/test/segment.ts",
    "test:segment:worker": "npm run rebuild && cross-env TS_NODE_PROJECT=tsconfig.json TS_NODE_FILES=true ts-node ./src/test/segment-worker.ts"
  },
